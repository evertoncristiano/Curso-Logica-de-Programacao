algoritmo "semnome"
// Função :
// Autor :
// Data : 30/04/2016
// Seção de Declarações 
var

   indice:inteiro
   nome:vetor[1..5] de caracter

inicio
// Seção de Comandos
indice<-1

  repita
  escreval("Digite o seu nome")
  leia (nome[indice])
  indice<- indice+1
  escreval(indice,") ",nome)
  leia (nome[indice])


 ate indice = 3
 limpatela
  escreval(indice,") ",nome)


 
fimalgoritmo
